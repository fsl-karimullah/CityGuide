    "concurrently": "^7.2.0",
    "eslint": "^8.14.0",
    "jest": "^28.0.3",
    "metro-react-native-babel-preset": "^0.70.2",
    "postcss": "^8.4.13",
    "react-test-renderer": "17.0.2",
    "tailwindcss": "^3.0.24"
  },
  "jest": {
    "preset": "react-native"
